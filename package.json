    "eslint": "8.0.1",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-n": "15.0.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.0.0",
    "nodemon": "2.0.20",
    "prettier": "2.7.1",
    "prettier-plugin-organize-imports": "3.2.0",
    "ts-node": "10.9.1",
    "typescript": "4.8.4"
